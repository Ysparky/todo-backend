{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "error",
            "single"
          ],
          "semi": [
            "error",
            "always"
          ],
          "no-console": 0,
          "no-unused-vars": 0
    }
}
